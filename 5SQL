SELECT * FROM products WHERE company NOT IN ('HTC');
SELECT * FROM products WHERE company = 'Apple' AND price < 75000;
SELECT * FROM products WHERE price >= 46000;
SELECT productName, price FROM products ORDER BY price DESC LIMIT 2;
SELECT productName, price FROM products ORDER BY price ASC LIMIT 1;
SELECT productName, isDiscounted FROM products WHERE isDiscounted = 'true';
SELECT productName, isDiscounted FROM products WHERE isDiscounted = 'false';
SELECT productName, price FROM products ORDER BY price DESC;
SELECT SUM(price * productCount) FROM products;
SELECT company, COUNT(*) AS modelsCount FROM products GROUP BY company HAVING COUNT(*) <= 2;
